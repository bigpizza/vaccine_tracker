{
  "name": "Safeway Vaccine Tracker",
  "description": "Track COVID-19 vaccine availability at Safeway around you.",
  "version": "1.1",
  "manifest_version": 2,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {},
  "permissions": [
    "webRequest",
    "notifications",
    "alarms",
    "tabs",
    "activeTab",
    "https://www.mhealthappointments.com/covidappt"
  ]
}